package poly;

public class sample {
	

	public void addnumbers(int  size)
	{
		try{
		int[] arr={12,13,14,15,45};
		
		for(int i=0;i<size;i++);
		{
	
			int i;
			System.out.println(arr[i]);
		}
		}catch(Exception e){
			System.out.println("index less then 4");
		}
	}

	public static void main(String[] args) {
		sample s=new sample();
		s.addnumbers(3);

	}

}
